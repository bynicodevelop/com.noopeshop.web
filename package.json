{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "export FIRESTORE_EMULATOR_HOST='localhost:8080' && export FIREBASE_AUTH_EMULATOR_HOST='127.0.0.1:9099' && export FIREBASE_STORAGE_EMULATOR_HOST='0.0.0.0:9199' && nuxt dev",
    "clean": "rm -rf .nuxt/ .output/ node_modules/ .firebase/",
    "preset": "NITRO_PRESET=firebase yarn build",
    "local": "NITRO_PRESET=firebase yarn build && firebase emulators:start",
    "deploy": "yarn clean && yarn && yarn preset && cd .output/server && rm -rf node_modules && npm i && cd ../../ && firebase deploy",
    "generate": "nuxt generate",
    "preview": "nuxt preview"
  },
  "devDependencies": {
    "@nuxt/content": "^2.0.1",
    "@nuxtjs/tailwindcss": "^5.1.2",
    "firebase-functions-test": "^2.2.0",
    "nuxt": "3.0.0-rc.3"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.4",
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.2",
    "browser-image-compression": "^2.0.0",
    "compressorjs": "^1.1.1",
    "firebase": "^9.8.3",
    "firebase-admin": "^10.3.0",
    "firebase-functions": "^3.21.2",
    "md5": "^2.3.0",
    "sparkpost": "^2.1.4",
    "vue-scrollto": "^2.20.0",
    "yup": "^0.32.11"
  }
